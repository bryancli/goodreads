<div class="page-header">
  <h1><%= @review.book.title %>'s Review</h1>
</div>

<dl class="dl-horizontal">

  <!-- <dt>Book</dt>
  <dd><%= @review.book_id %></dd> -->


  <dt>User</dt>
  <dd><%= @review.user.username %></dd>


  <dt>Summary</dt>
  <dd><%= @review.summary %></dd>


  <dt>Fullreview</dt>
  <dd><%= @review.fullReview %></dd>


  <dt>Startdate</dt>
  <dd><%= @review.startDate %></dd>


  <dt>Enddate</dt>
  <dd><%= @review.endDate %></dd>


  <dt>Enlightenmentrating</dt>
  <dd><%= @review.enlightenmentRating %>/5</dd>


  <dt>Enjoymentrating</dt>
  <dd><%= @review.enjoymentRating %>/5</dd>


  <dt>Overallrating</dt>
  <dd><%= @review.overallRating %>/5</dd>


</dl>
